<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@+id/navigation_statistic">

    <fragment
        android:id="@+id/navigation_statistic"
        android:name="com.project.sleepwell.fragment.StatisticFragment"
        android:label="fragment_statistic"
        tools:layout="@layout/fragment_statistic" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.project.sleepwell.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.project.sleepwell.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_results"
        android:name="com.project.sleepwell.fragment.ResultsTrackingFragment"
        android:label="fragment_results_tracking"
        tools:layout="@layout/fragment_results_tracking" />

</navigation>